{
  "name": "git-trust-me-bro",
  "displayName": "GitTrustMeBro",
  "description": "",
  "version": "0.0.1",
  "publisher": "sepia",
  "repository": "https://github.com/MarcSepia/git-trust-me-bro.git",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "activationEvents": [
    "onView:scm"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GitTrustMeBro.commit",
        "title": "Commit (no-verify)",
        "category": "Git (No Verify)"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "GitTrustMeBro.commit",
          "group": "2_commit",
          "when": "scmProvider == git"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}